# 모듈(module) 만들기
class Character:
    def __init__(self, name, health):
        self.name = name
        self.health = health

    def attack(self):
        print(f"{self.name}이(가) 공격합니다!!")


%%writefile character.py
class Character:
    def __init__(self, name, health):
        self.name = name
        self.health = health

    def attack(self):
        print(f"{self.name}이(가) 공격합니다!!")


