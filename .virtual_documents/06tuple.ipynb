# tuple: 시퀀스 자료형 중 하나, 불변(immutable)
# 변수 = (항목1, 항목2, 항목3, ...)
x = (1, 2, 3, 4, 5, 6)


print(x)


print(type(x))


x = ()


print(type(x))


print(len(x))


print(x)


y = tuple()


print(y)


print(type(y))


x1 = (1, 2, 3, 4, 5)


# x1[1] = 3과 같이 수정 불가능
print(x1[:2])


x2 = "음", "오", "아", "예"


print(type(x2))


print(x2)



